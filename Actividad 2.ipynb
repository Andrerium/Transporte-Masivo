    "# Creamos un DF de prueba con las nodos y la información de las mismas\n",
    "data = {\n",
    "    'Origen': ['A', 'A', 'B', 'B', 'C', 'C', 'D', 'D', 'E', 'F'],\n",
    "    'Destino': ['B', 'C', 'A', 'D', 'A', 'E', 'C', 'F', 'D', 'D'],\n",
    "    'Distancia': [5, 10, 5, 5, 15, 5, 10, 5, 10, 5]\n",
    "}\n",//
    "\n",
    "df = pd.DataFrame(data)\n",
